{
    "plugins": ["prettier-plugin-tailwindcss"],
    "tabWidth": 4,
    "singleQuote": true,
    "semi": true,
    "printWidth": 80,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "jsxSingleQuote": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed"
}
